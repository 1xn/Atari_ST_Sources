#!make -f
# @(#)windom.mk, 
#
# make file for 'WinDOM' demo program
#

all:
	@echo makefile for the demo
	@echo targets are clean, sox, gcc or gcc281

.PHONY : clean sox gcc281 gcc

clean:
	rm -f *.o

sox:
	make -f m_sox

gcc281:
	make -f m_gcc281

gcc:
	make -f m_gcc

#eof
