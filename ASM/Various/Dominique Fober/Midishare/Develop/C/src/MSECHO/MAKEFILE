CFLAGS = -DMWC -A
LIB   = -lmidi
OBJ   = midiecho.o msh_echo.o rsc_echo.o

midiecho: $(OBJ)
       cc -o msecho.prg -VGEMAPP $(OBJ) $(LIB)
       strip msecho.prg
       cp msecho.prg msecho.acc

midiecho.o: msecho.h echo.h
rsc_echo.o: msecho.h echo.h
msh_echo.o: echo.h
