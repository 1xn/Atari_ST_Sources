lx%=32
ly%=16
plx%=DIV(lx%,16)
plan%=1
OPEN "o",#1,"code.lst"
FOR o%=0 TO plx%-1
  FOR i%=0 TO 198
    IF plan%=1
      PRINT #1,"card{i1%+";i%*160+o%*8;"}=card{i1%+";(i%+1)*160+o%*8;"}"
    ELSE IF plan%=2
      PRINT #1,"card{i1%+";i%*160+o%*8;"}=card{i1%+";(i%+1)*160+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+2+o%*8;"}=card{i1%+";(i%+1)*160+2+o%*8;"}"
    ELSE IF plan%=3
      PRINT #1,"card{i1%+";i%*160+o%*8;"}=card{i1%+";(i%+1)*160+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+2+o%*8;"}=card{i1%+";(i%+1)*160+2+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+4+o%*8;"}=card{i1%+";(i%+1)*160+4+o%*8;"}"
    ELSE IF plan%=2
      PRINT #1,"card{i1%+";i%*160+o%*8;"}=card{i1%+";(i%+1)*160+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+2+o%*8;"}=card{i1%+";(i%+1)*160+2+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+4+o%*8;"}=card{i1%+";(i%+1)*160+4+o%*8;"}"
      PRINT #1,"card{i1%+";i%*160+6+o%*8;"}=card{i1%+";(i%+1)*160+6+o%*8;"}"
    ENDIF
  NEXT i%
  PRINT #1,"."
NEXT o%
CLOSE #1
