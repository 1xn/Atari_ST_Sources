> PROCEDURE user_rsc_interact(index&,tree&,object&,mc&,sub_me&)
'
'  <index&> Index des Fensters in window_array&(index&,x)
'           Wenn das Objekt aus der MenÅzeile ist: <index&>=-1
'   <tree&> Index des Objektbaumes
' <object&> Index des selektierten Objektes (Åber Mausklick oder Shortcut)
'     <mc&> Anzahl der Mausklicks (1=normal oder Shortcut / 2=Doppelklick)
' <sub_me&> ausgewÑhlter MenÅeintrag in Popup-MenÅs
'
SELECT tree&
'
' ------------------------------------------------------------------------
'
CASE coffee1&
SELECT object&
CASE startok&
test&=@win_open_dialog(2,coffee2&,-1)
@win_close_dialog(coffee1&)
ENDSELECT
'
' ------------------------------------------------------------------------
'
CASE coffee2&
SELECT object&
CASE genau&
~@win_open_dialog(2,main&,-1)
@win_close_dialog(coffee2&)
ENDSELECT
'
' ------------------------------------------------------------------------
'
CASE main&
SELECT object&
CASE kaffeepar&,mitmilch&,mitmilchu&,mitzucker&,sonstnoch&
@call_gemjing(program_path$+"clunk.wav")
CASE goldfilter&
IF goldfilter_var!=TRUE
@call_gemjing(program_path$+"checka.wav")
ELSE
@call_gemjing(program_path$+"checkd.wav")
ENDIF
CASE aroma&
IF aroma_var!=TRUE
@call_gemjing(program_path$+"checka.wav")
ELSE
@call_gemjing(program_path$+"checkd.wav")
ENDIF
CASE entkoff&
IF entkoff_var!=TRUE
@call_gemjing(program_path$+"checka.wav")
ELSE
@call_gemjing(program_path$+"checkd.wav")
ENDIF
CASE slider&
CASE info&
~@win_open_dialog(2,about&,-1)
CASE start&
@pruuueett
~@win_open_dialog(2,coffeeready&,-1)
@win_close_dialog(main&)
ENDSELECT
'
' ------------------------------------------------------------------------
'
CASE coffeeready&
SELECT object&
CASE ahh&
@win_close_dialog(coffeeready&)
exit_program!=TRUE
ENDSELECT
'
' ------------------------------------------------------------------------
'
CASE about&
SELECT object&
CASE infoclose&
@win_close_dialog(about&)
ENDSELECT
'
' ------------------------------------------------------------------------
'
'
' ÍÍwsnippetÍÍ  - Wrinkle-Code: (dieses Flag nicht lîschen oder verÑndern)
' ÍÍwsnippetÍÍ  - Ende des Wrinkle-Codes: (dieses Flag nicht lîschen oder verÑndern)
'
ENDSELECT
RETURN
