> PROCEDURE pruuueett
@progress_open("PrÅtt... PrÅtt... PrÅtt...",TRUE)
FOR a&=0 TO 10
  '
  ' Hier wird krÑftig gerechnet...
  '
  DELAY 0.5
  IF a&=5
    @call_gemjing(program_path$+"prueet.wav")
  ENDIF
  EXIT IF @progress_update(INT(a&/10))
NEXT a&
@progress_close
RETURN
