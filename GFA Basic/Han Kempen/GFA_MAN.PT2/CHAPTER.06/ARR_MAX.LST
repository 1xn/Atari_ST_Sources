' *** ARR_MAX.LST ***                                  2nd GFA-Manual, page 6-8
FUNCTION array_max(VAR proc())
  '
  ' *** Return highest value in word-array
  '
  LOCAL i,high
  high=proc(0)
  FOR i=1 TO PRED(DIM?(proc()))
    high=MAX(high,proc(i))
  NEXT i
  RETURN high
ENDFUNC
' *****
'
