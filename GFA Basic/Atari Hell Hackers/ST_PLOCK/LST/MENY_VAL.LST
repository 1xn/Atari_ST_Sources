' exempel:
DIM entry$(20)
DATA "  Desk  ","  Test  "
DATA ----------------,1,2,3,4,5,6,""
DATA "  File  ","  Load  ","  Save  "
DATA ---------,"  Quit  ",""
DATA "  Titles  ","  Entry 1  ","  Entry 2  ",""
DATA End
i%=-1
REPEAT
  INC i%
  READ entry$(i%)
UNTIL entry$(i%)="End"
entry$(i%)=""
MENU entry$()
ON MENU GOSUB evaluate
OPENW 0
REPEAT
  ON MENU
UNTIL MOUSEK=2
PROCEDURE evaluate
  MENU OFF
  m%=MENU(0)
  PRINT entry$(m%)
  ALERT 0,"Bock f”re rad?",0,"JA|NEJ",a%
  IF a%=1
    MENU m%,1
  ELSE
    MENU m%,0
  ENDIF
  ALERT 0,"Ljusa tecken|(ej valbara)",0,"JA|NEJ",a%
  IF a%=1
    MENU m%,2
  ELSE
    MENU m%,3
  ENDIF
RETURN
