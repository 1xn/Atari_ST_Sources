<DOKU>

FUNCTION str_readmem$(adr%,len&)        ! Text aus Speicher auslesen
  ' 30-09-94
  LOCAL txt$                    !
  '
  IF len&<32767 AND len&>0      ! Nur falls sinnvolle L„ngenangaben
    txt$=SPACE$(len&)           !
    BMOVE adr%,V:txt$,len&      !
  ENDIF                         !
  '
  RETURN txt$                   !
ENDFUNC
