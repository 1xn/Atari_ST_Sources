<DOKU>

FUNCTION mem_is(start%,len%,asc&)               ! Besteht der Speicherblock nur aus asc&-Zeichen ?
  ' 30-06-94
  LOCAL a%                              !
  '
  FOR a%=0 TO len%-1                    !
    IF BYTE{start%+a%}<>asc&            !
      RETURN FALSE                      !
    ENDIF                               !
  NEXT a%                               !
  '
  RETURN TRUE                           !
ENDFUNC
