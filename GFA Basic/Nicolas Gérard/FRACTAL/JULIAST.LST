SETCOLOR 0,0
SETCOLOR 1,&H200
SETCOLOR 2,&H300
SETCOLOR 3,&H400
SETCOLOR 4,&H510
SETCOLOR 5,&H620
SETCOLOR 6,&H740
SETCOLOR 7,&H750
SETCOLOR 8,&H457
SETCOLOR 9,&H247
SETCOLOR 10,&H236
SETCOLOR 11,&H225
SETCOLOR 12,&H214
SETCOLOR 13,&H103
SETCOLOR 14,&H2
SETCOLOR 15,0
FOR y=0 TO 99
  FOR x=0 TO 319
    u=x/100-1.6
    v=y/100-1
    FOR i=0 TO 50
      e=u
      u=u^2-v^2-0.4
      v=2*e*v+0.7
      n=i
      IF u^2+v^2>4
        i=100
      ENDIF
    NEXT i
    COLOR n
    PSET x,y,n
    PSET 319-x,199-y,n
  NEXT x
NEXT y
VSETCOLOR 0,15,15,15
VSETCOLOR 1,0,0,0
