Datum$="26.09.1987"
Cls
Anfang=Timer
Stufe=0
Tippzahl=0
Dim A$(150),X1(150),X2(150),Y1(150),Y2(150)
Dim Stufe(150),Art$(150),Shift(150),Finger(150)
Dim Kleinkon$(50),Grosskon$(50),Zeich$(50),Vok$(50)
Dim Finger$(9),Pfeil_x(9),Pfeil_y(9)
